#########################################
##  Colors from color0 to color254 can be set  ##
#########################################

## black
color0 = #2d2d2d
color8 = #606060

## red
color1 = #993a3b
color9 = #c25e5f

## green
color2 = #82994f
color10 = #9bb26a

## orange
color3 = #d28445
color11 = #f4bf77

## blue
color4 = #5994ad
color12 = #7baabd

## magenta
color5 = #a06594
color13 = #b485aa

## cyan
color6 = #64aca0
color14 = #86beb4

## white
color7 = #d0d0d0
color15 = #f5f5f5
